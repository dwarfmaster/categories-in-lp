DECLARE PLUGIN "diagrams_plugin"

{

open Pp
open Ltac_plugin
open Stdarg

}

TACTIC EXTEND hello_world_tactic
| [ "hello" ] ->
  { let _ = Feedback.msg_notice (strbrk Main.message) in
    Tacticals.tclIDTAC }
END

VERNAC COMMAND EXTEND diagram_print CLASSIFIED AS QUERY
| ![ proof_query ] [ "print_diagram" string(file) ] ->
  { fun ~pstate ->
    let state = Declare.Proof.get pstate in
    Extractor.extract state file
  }
END
